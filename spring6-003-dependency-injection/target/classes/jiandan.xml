<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="jiandanBean" class="com.powernode.spring6.service.jiandanService">
        <constructor-arg name="age" value="十二"></constructor-arg>
        <constructor-arg name="num" value="12"></constructor-arg>
        <property name="age" value="十二"></property>
        <property name="num" value="12"></property>
    </bean>
    <bean id="dogBean" class="com.powernode.spring6.service.dog" c:age="18" c:name="Ciallo">
    </bean>
</beans>